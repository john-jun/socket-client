{
    "name": "john-jun/socket-client",
    "description": "A simple socket client library",
    "keywords": ["Air", "Socket", "Socket Client"],
    "license": "MIT",
    "authors": [
        {
            "name": "John",
            "email": "53950451@qq.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "scripts": {
        "test": "phpunit --colors=always tests"
